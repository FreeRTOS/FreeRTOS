    <processsectorsindividually>false</processsectorsindividually>
  </eraseblankcheckwindow>

  <checksumwindow>
    <computechecksumover>FileOnTarg</computechecksumover>
    <addrstart>0x00000000</addrstart>
    <addrsize>0x0000FFFF</addrsize>
  </checksumwindow>

</fpconfig>
