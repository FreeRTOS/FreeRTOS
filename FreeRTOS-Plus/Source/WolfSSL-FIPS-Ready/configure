 { $as_echo "$as_me:${as_lineno-$LINENO}: checking for ippsRSAEncrypt_PKCSv15 in -lippcp" >&5
$as_echo_n "checking for ippsRSAEncrypt_PKCSv15 in -lippcp... " >&6; }
